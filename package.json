{
  "name": "react-webpack-boilerplate",
  "version": "2.1.2",
  "description": "A React and Redux development framework.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-dev": "rimraf build",
    "dev": "cross-env NODE_ENV=development babel-node server.js 8877",
    "build-dev": "npm run clean-dev && cross-env NODE_ENV=development webpack -d --progress --profile --colors --config webpack.config.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --progress --config webpack.config.js"
  },
  "author": "Alan Gao",
  "license": "MIT",
  "repository": {
